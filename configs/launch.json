{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) Attach",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bazel-bin/source/exe/envoy-static",
        "MIMode": "gdb",
        "cwd": "${workspaceFolder}",
        "miDebuggerServerAddress": "localhost:9999",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    },
}

